@{
    Page.Title = "Title goes here";
}

<style>
    #map {
        height: 400px;
        width: 400px;
    }
</style>

<div class="row">
    <div id="map"></div>
    <script>
        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: -34.397, lng: 150.644 },
                zoom: 8
            });
        }
    </script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDeSg65dJVzuwzWhMeWvu5lM7Xna0pYMAY&callback=initMap"
            type="text/javascript"></script>
</div>

